<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body style="background-color: rgb(247, 242, 242);">
    <%- include('./partials/nav.ejs') %>
    <div class="content">
      <h1>Prices</h1>
      <% for (let i = 0; i < prices.length; i++) { %>
        <div style="background-color: rgb(204, 236, 209); margin-bottom: 10px;">
          <h4><%= prices[i].name %>:</h4>
          <p>Price: <%= prices[i].price %> USD</p>
        </div>
      <% } %> 
    <%- include('./partials/footer.ejs') %>
  </body>
</html>



